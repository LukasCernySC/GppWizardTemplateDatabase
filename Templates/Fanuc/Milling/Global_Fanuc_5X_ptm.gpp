      endif
   endif

   if bVMID_MO_FineShiftAfterRot ;TPL 3.0
      {nb,(52):gcode_f 'X#'iA_FineShiftParameter<<home_id,1>> ' Y#'iA_FineShiftParameter<<home_id,2>> ' Z#'iA_FineShiftParameter<<home_id,3>>}
