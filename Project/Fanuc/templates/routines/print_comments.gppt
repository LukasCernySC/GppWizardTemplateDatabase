@usr_PrintMessage(integer iL_MessageCounter, string sL_ToDo, string sL_CallRoutine)

	local string sLA_Message<<61>> sL_RoutineMessage
	sL_ToDo = upper(sL_ToDo)

	if instr(sL_ToDo,'{{res.abort}}') eq 0 and instr(sL_ToDo,'{{res.hint}}') eq 0  and instr(sL_ToDo,'{{res.message}}') eq 0
      {{if res.language == "de"-}}
      sL_RoutineMessage = 'Im Fehlerroutinenaufruf #'+tostr(iL_MessageCounter)+' aus '+sL_CallRoutine+' ist der zweite Parameter falsch deklariert !'
      {{else-}}
      	{{if res.language == "en"-}}
      	sL_RoutineMessage = 'For @usr_PrintMessage(('+tostr(iL_MessageCounter)+'),('+sL_ToDo+'),('+sL_CallRoutine+')) the 2nd parameter is wrong declared!'
      	{{end-}}
      {{end-}}
      print sL_RoutineMessage
      abort
   endif
 
   sLA_Message<<1>>  = '{{res.sLA_Message1}}' 
   sLA_Message<<2>>  = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'{{res.sLA_Message2_1}}'
   sLA_Message<<3>>  = '' ;not used
   sLA_Message<<4>>  = '{{res.sLA_Message4}}'
   sLA_Message<<5>>  = '{{res.sLA_Message5}}'
   sLA_Message<<6>>  = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'{{res.sLA_Message6}}'
   sLA_Message<<7>>  = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): @drill_4x_pnt noch nicht getestet!'
   sLA_Message<<8>>  = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Abwicklung mit KopfKopf-Kinematik nicht mï¿½glich'
   sLA_Message<<9>>  = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Die NP-Positionen dï¿½rfen bei 3X-Ausgabe nicht verwendet werden'
   sLA_Message<<10>> = 'PP/CAM: Ein Wechsel der SUB-MASCHINE zwischen Jobs ist nicht erlaubt! Kann aber, wenn benï¿½tigt, mit "b_SubMachineChange" erlaubt werden.'
   sLA_Message<<11>> = 'PP/VMID: Im Job: '+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Bei Verwendung der Loops muss der VMID-Parameter "Unterprogramme" aktiviert werden'
   sLA_Message<<12>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Fehlerhafter Adressraum angegeben fï¿½r G28'
   sLA_Message<<13>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Bei Bohrzyklus "'+drill_cycle_name+'" sollte ein Gewindebohrer benutzt werden'
   sLA_Message<<14>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Unter Verschiedene Job-Parameter "3D-Kompensation" muss definiert werden'
   sLA_Message<<15>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Das Format beim Zyklus G84/G74 mit M29 sollte ï¿½berprï¿½ft werden'
   sLA_Message<<16>> = job_name+': Sie kï¿½nnen optional auch unter Bewegungseingrenzung --> die 4.Achse ausschalten.'
   sLA_Message<<17>> = 'PP: Error! Check VMID definition. Dummy device is wrong!'
   sLA_Message<<18>> = 'PP: Schwenkreihenfole Kinematik-Achsen und 45-Grad Kinematik nicht mï¿½glich, auf Benutzerdefiniert wechseln!'
   sLA_Message<<19>> = 'PP/VMID: HSC-Option (G05.1 oder G121 in den Maschinenoptionen nicht gesetzt!'
   sLA_Message<<20>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): HSC-Stufe (P1 oder M3 usw) in den Joboptionen nicht gesetzt!'
   sLA_Message<<21>> = 'Die Bruchkontrolle nicht integriert. Bitte wenden Sie sich an den SolidCAM Support'
   sLA_Message<<22>> = '{{res.sLA_Message2}}'+job_name+' (JobNr.:'+tostr(index_job:'5.0(n)')+'): Bei der Spiegelung um nur einer Achse ist die Kreisbewegung und die Korrekturrichtung umgekehrt'
   sLA_Message<<23>> = 'Die maximale programmierbare Nullpunktnummer ï¿½berschritten. Bitte prï¿½fen Sie die GPP-Parameter "i_MRZPointSavedIn"'
   sLA_Message<<24>> = 'Die maximale programmierbare Nullpunktnummer ï¿½berschritten. Bitte prï¿½fen Sie die GPP-Parameter "i_CalcHomeSavedIn"'
   sLA_Message<<25>> = 'PP: Schwenkreihenfolge fï¿½r G68.2 nicht zulï¿½ssig'
   
   ;'\x09' = TAP
   sL_RoutineMessage = sLA_Message<<iL_MessageCounter>> + '\x0a' + '\x0a' + ';.....Meldung Nr.'+tostr(iL_MessageCounter)+' Aufruf aus '+sL_CallRoutine

   if instr(sL_ToDo,'{{res.message}}') ne 0
      if sL_CallRoutine ne ''
         print sL_RoutineMessage
      else
         print sLA_Message<<iL_MessageCounter>>
         sL_RoutineMessage = sLA_Message<<iL_MessageCounter>>
      endif 
      
      if sL_CallRoutine ne ''
         sL_RoutineMessage = sLA_Message<<iL_MessageCounter>> + '\x0a' + '; Meldung Nr.'+tostr(iL_MessageCounter)+' Aufruf aus '+sL_CallRoutine ;TPL 4.0
      endif
   endif
   
   if instr(sL_ToDo,'{{res.abort}}') ne 0 or instr(sL_ToDo,'{{res.hint}}') ne 0
      {nl,';--------------------------------------------'}
      {nl,'; ' sL_RoutineMessage}
      {nl,';--------------------------------------------'}
      {nl}
      if instr(sL_ToDo,'{{res.abort}}') ne 0
         {nl,';--- Abbruch der NC-Erzeugung ---'}
         abort
      endif
   endif

endp

