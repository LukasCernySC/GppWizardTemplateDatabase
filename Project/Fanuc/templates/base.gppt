	endif
	if current_command eq '@arc_5x' or current_command eq '@line_5x'
		if bA_CylindricalInterpolation<<ChannelID>> eq true
			nA_SaveLast_CylindricalCartesianY<<ChannelID>> = nA_CylindricalCartesianY_RAW<<ChannelID>>
		endif
	endif
